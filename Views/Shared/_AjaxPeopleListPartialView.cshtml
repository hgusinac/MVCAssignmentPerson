@model List<Person>

<div class="row">
    <div class="col-12">
        <div class="row">

            <div class="col-3">
                <h5>Name</h5>
            </div>

            <div class="col-3">
                <h5>Phone</h5>
            </div>

            <div class="col-3">
                <h5>City</h5>
            </div>

            <div class="col-3">
                <h5> Id </h5>
            </div>

        </div>

        @foreach (var item in Model)
        {
            <partial name="_AjaxPersonPartialView" model="item" />
        }

    </div>

</div>
